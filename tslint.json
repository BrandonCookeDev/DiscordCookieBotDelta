{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"ordered-imports": false,
		"object-literal-sort-keys": false,
        "eofline": false, 
		"whitespace": false,
		"one-line": false,
		"semicolon": false,
		"curly": false,
		"indent":[
			true,
			"tabs",
			4
		],
        "quotemark": [
            true,
            "single"
        ]

	},
	"rulesDirectory": []
}